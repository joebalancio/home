{
  "session_name": "bc",
  "windows": [
    {
      "focus": true,
      "layout": "even-vertical",
      "panes": [
        "cd ~/projects/buildingconnected/BC",
        "cd ~/projects/buildingconnected/client",
        "cd ~/projects/buildingconnected/printer"
      ],
      "window_name": "Servers"
    },
    {
      "layout": "even-horizontal",
      "panes": [
        null,
        "docker ps && yarn start"
      ],
      "shell_command_before": [
        "cd ~/projects/buildingconnected/env"
      ],
      "window_name": "Env"
    },
    {
      "panes": [
        null
      ],
      "shell_command_before": [
        "cd ~/projects/buildingconnected/client"
      ],
      "window_name": "Client"
    },
    {
      "panes": [
        null
      ],
      "shell_command_before": [
        "cd ~/projects/buildingconnected/BC"
      ],
      "window_name": "BC"
    },
    {
      "panes": [
        null
      ],
      "shell_command_before": [
        "cd ~/projects/buildingconnected/stanbot"
      ],
      "window_name": "Stanbot"
    },
    {
      "panes": [
        null
      ],
      "window_name": "Heroku"
    },
    {
      "panes": [
        {
          "focus": true,
          "shell_command": "node"
        }
      ],
      "window_name": "Playground"
    }
  ],
  "global_options": {
    "base-index": 1,
    "pane-base-index": 1
  }
}
